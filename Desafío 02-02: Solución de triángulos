using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp2
{
    class Program
    {
        static void Main(string[] args)
        {
            Console.WriteLine("Ingrese t: ");
            double t = double.Parse(Console.ReadLine());
            Console.WriteLine("Ingrese a: ");
            double a = double.Parse(Console.ReadLine());
            double c = 90 - a;
            
            double y = Math.Sin(a *  Math.PI /180)*t;
            double z = Math.Sin(c* Math.PI / 180)*t;
            
            Console.WriteLine("el valor t es " + t);
            Console.WriteLine("el valor a es " + a);
            Console.WriteLine("el valor c encontrado es " + c);
            Console.WriteLine("el valor y encontrado es " + y);
            Console.WriteLine("el valor z encontrado es " + z);
        }
    }
}
